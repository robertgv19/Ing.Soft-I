@model Ing.soft_I.Models.venta

<div class="row">
    <div class="col-md-4">


       <form asp-controller="Venta" asp-action="Editarr">

           <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">id_Venta</label>
    <input asp-for="idventa" value="@Model.idventa" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required="true">
  </div>

      <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Fecha de Venta</label>
    <input asp-for="fechaventa" value="@Model.fechaventa" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required="true">
  </div>

    <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Descripcion de Venta</label>
    <input asp-for="descventa" value="@Model.descventa" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required="true">
  </div>

    <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Precio venta</label>
    <input asp-for="precio" value="@Model.precio" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required="true">
  </div>
    <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Documento de cliente</label>
    <input asp-for="cliente_documento" value="@Model.cliente_documento" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required="true">
  </div>

  <button type="submit" class="btn btn-primary">Modificar Insumo</button>
  </form>
    </div>
</div>